<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE HLScheme>
<HLScheme>
	
	<scheme name="INI" extensions="ini">
		<rule>
			<expr>^\[.*\]</expr>
			<style color="#000099" bold="true" />
		</rule>
		<rule>
			<expr>^[\s\t]*\w*[\s\t]*\=</expr>
			<style color="#006600" />
		</rule>
		<rule>
			<expr>\=(.*$)</expr>
			<style entry="1" color="#990000" />
		</rule>
	</scheme>
	
</HLScheme>